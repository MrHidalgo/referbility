.posting__btn-wrapper.is-active(posting-action-js data-wrapper-id='1')
  div
  a.posting__btn.c-btn.c-btn--bg.c-btn--bg-gradient(href="#" data-id='2' posting-next-js)
    span Next

.posting__btn-wrapper(posting-action-js data-wrapper-id='2')
  a.posting__btn.c-btn.c-btn--bg.c-btn--bg-gray(href="#" data-id='1' posting-back-js)
    span Back
  a.posting__btn.c-btn.c-btn--bg.c-btn--bg-gradient(href="#" data-id='3' posting-next-js)
    span Next

.posting__btn-wrapper(posting-action-js data-wrapper-id='3')
  a.posting__btn.c-btn.c-btn--bg.c-btn--bg-gray(href="#" data-id='2' posting-back-js)
    span Back
  a.posting__btn.c-btn.c-btn--bg.c-btn--bg-gradient(href="#" data-id='4' data-init='range' posting-question-js)
    span Next

.posting__btn-wrapper(posting-action-js data-wrapper-id='4')
  a.posting__btn.c-btn.c-btn--bg.c-btn--bg-gray(href="#" data-id='3' posting-back-js)
    span Back
  a.posting__btn.c-btn.c-btn--bg.c-btn--bg-gradient(href="#postingReview" data-name='Change Plan' data-effect='mfp-zoom-in' posting-review-js)
    span Choose Plan
  a.posting__btn.c-btn.c-btn--bg.c-btn--bg-gradient(href="#" data-id='5' posting-question-js)
    span Review

.posting__btn-wrapper(posting-action-js data-wrapper-id='5')
  a.posting__btn.c-btn.c-btn--bg.c-btn--bg-gray(href="#" data-id='4' posting-back-js)
    span Back
  a.posting__btn.c-btn.c-btn--bg.c-btn--bg-gradient(href="#postingPost" data-effect='mfp-zoom-in' popup-js)
    span Post
